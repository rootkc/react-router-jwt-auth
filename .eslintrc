{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [2, {"extensions": [".js", ".jsx"]}],
    "semi": [2, "never"],
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "to" ],
    }]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
  },
  "settings": {
    "import/core-modules": [
      "components",
      "containers",
      "store",
      "services",
      "store/actions",
    ],
  }
}
