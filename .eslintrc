{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [2, {"extensions": [".js", ".jsx"]}],
    "semi": [2, "never"],
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
  },
  "settings": {
    "import/core-modules": [
      "components",
      "containers",
      "store",
      "store/actions",
    ],
  }
}
